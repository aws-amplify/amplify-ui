{
  "private": true,
  "name": "@aws-amplify/ui-environments",
  "version": "0.0.1",
  "workspaces": [
    "ai",
    "ai/*",
    "auth",
    "auth/*",
    "datastore",
    "datastore/*",
    "geo",
    "geo/*",
    "liveness",
    "liveness/*",
    "storage",
    "storage/*",
    "in-app-messaging",
    "in-app-messaging/*"
  ],
  "scripts": {
    "ai": "yarn --cwd ai/",
    "auth": "yarn --cwd auth/",
    "datastore": "yarn --cwd datastore/",
    "geo": "yarn --cwd geo/",
    "liveness": "yarn --cwd liveness/",
    "storage": "yarn --cwd storage/",
    "in-app-messaging": "yarn --cwd in-app-messaging/"
  }
}
