    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@size-limit/preset-app": "^7.0.8",
    "customize-cra": "^1.0.0",
    "react-app-rewired": "^2.2.1",
    "serve": "^13.0.2",
    "size-limit": "^7.0.8"
  },
  "size-limit": [
    {
      "path": "build/static/js/main.*.js",
      "limit": "281 kB"
    }
  ]
}
