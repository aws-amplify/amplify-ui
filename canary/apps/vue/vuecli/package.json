{
  "name": "vuecli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service --skip-plugins @vue/cli-plugin-eslint serve --port 3000",
    "build": "vue-cli-service --skip-plugins @vue/cli-plugin-eslint build",
    "lint": "vue-cli-service lint",
    "start": "serve dist -s -p 3000"
  },
  "dependencies": {
    "@aws-amplify/ui-vue": "latest",
    "aws-amplify": "latest",
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "serve": "^13.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
