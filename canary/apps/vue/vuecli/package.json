{
  "name": "vuecli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service --skip-plugins @vue/cli-plugin-eslint serve --port 3000",
    "build": "vue-cli-service --skip-plugins @vue/cli-plugin-eslint build",
    "lint": "vue-cli-service lint",
    "start": "serve dist -s -p 3000"
  },
  "dependencies": {
    "@aws-amplify/ui-vue": "latest",
    "aws-amplify": "latest",
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.3.6",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.17.0",
    "serve": "^13.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
