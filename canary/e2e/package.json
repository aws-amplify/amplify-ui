{
  "name": "canary-cypress",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf node_modules",
    "dev": "TZ=UTC cypress open",
    "test": "TZ=UTC cypress run --spec 'features/**/*.feature'"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "nonGlobalStepBaseDir": "cypress/integration",
    "stepDefinitions": [
      "cypress/integration/ui/[filepath]/*.steps.ts",
      "cypress/integration/common/*.ts"
    ]
  },
  "author": "",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.1.0",
    "@bahmutov/cypress-esbuild-preprocessor": "~2.1.3",
    "@testing-library/cypress": "^8.0.3",
    "cypress": "^10.7.0",
    "dotenv-safe": "^8.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "resolutions": {
    "tough-cookie": "4.1.3"
  }
}
