{
  "name": "canary",
  "version": "0.0.1",
  "scripts": {
    "test": "echo \"Future\" && exit 1",
    "build": "yarn react && yarn vue && yarn angular",
    "install": "yarn react:install && yarn vue:install && yarn angular:install",
    "react": "yarn cra && yarn cra-ts && yarn next",
    "react:install": "yarn cra:install && yarn cra-ts:install && yarn next:install",
    "vue": "yarn vite && yarn vuecli",
    "vue:install": "yarn vite:install && yarn vuecli:install",
    "angular": "yarn angularcli",
    "angular:install": "yarn angularcli:install",
    "cra": "yarn --cwd react/cra build ",
    "cra:install": "yarn --cwd react/cra install",
    "cra-ts": "yarn --cwd react/cra-ts build ",
    "cra-ts:install": "yarn --cwd react/cra-ts install",
    "next": "yarn --cwd react/next build ",
    "next:install": "yarn --cwd react/next install",
    "angularcli": "yarn --cwd angular/angularcli build ",
    "angularcli:install": "yarn --cwd angular/angularcli install",
    "vite": "yarn --cwd vue/vite build ",
    "vite:install": "yarn --cwd vue/vite install",
    "vuecli": "yarn --cwd vue/vuecli build ",
    "vuecli:install": "yarn --cwd vue/vuecli install"
  },
  "author": "",
  "license": "Apache-2.0"
}
