{
  "name": "canary-cypress",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf node_modules",
    "dev": "TZ=UTC cypress open",
    "test:web": "TZ=UTC cypress run --spec 'features/**/*.feature'"
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "stepDefinitions": [
      "cypress/integration/common/*.ts"
    ]
  },
  "author": "",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@testing-library/cypress": "^10.0.3",
    "cypress": "^14.0.3",
    "dotenv-safe": "^8.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "**/cypress-cucumber-preprocessor/**/semver": "6.3.1",
    "**/cypress-cucumber-preprocessor/**/core-js-compat/**/semver": "7.5.4",
    "tough-cookie": "4.1.3"
  }
}
