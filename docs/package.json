{
  "private": true,
  "name": "@aws-amplify/ui-docs",
  "version": "0.0.1",
  "scripts": {
    "dev": "yarn prebuild && yarn next dev -p 5001",
    "preflutter:build": "yarn flutter:clean",
    "flutter:build": "(cd flutter/authenticator && flutter build web)",
    "postflutter:build": "yarn flutter:copy",
    "flutter:clean": "rm -rf public/flutter/authenticator",
    "flutter:copy": "cp -r ./flutter/authenticator/build/web public/flutter/authenticator",
    "prebuild": "yarn build:cssvars-table && yarn build:props-tables",
    "build": "NODE_OPTIONS=\"--max-old-space-size=4096\" next build",
    "build:cssvars-table": "node --require esbuild-register ./scripts/generate-cssvars-table-data.ts",
    "build:props-tables": "node --require esbuild-register ./scripts/generate-props-tables-data.ts",
    "clean": "rimraf node_modules .next && yarn flutter:clean",
    "lint": "next lint",
    "postbuild": "yarn postbuild:sitemap && yarn test",
    "postbuild:previews": "node --require esbuild-register ./scripts/generate-previews.ts",
    "postbuild:sitemap": "node --require esbuild-register ./scripts/generate-sitemap-robotstxt.ts",
    "start": "next start",
    "test": "jest",
    "test:links": "node --require esbuild-register ./scripts/link-checker-puppeteer.ts"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "6.1.6",
    "@docsearch/react": "3",
    "@mdx-js/loader": "^2.1.0",
    "@mdx-js/mdx": "^2.1.0",
    "@mdx-js/react": "^2.1.0",
    "aws-amplify": "^6.0.6",
    "countries-list": "^2.6.1",
    "dotenv": "^16.0.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "mdx-prism": "^0.3.3",
    "next": "^13.5.5",
    "next-plugin-preval": "^1.2.1",
    "prism-react-renderer": "1.2.1",
    "raw-loader": "^4.0.2",
    "react": "18.2.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "18.2.0",
    "react-icons": "^4.3.1",
    "react-live": "^2.4.1",
    "rehype": "^11.0.0",
    "rehype-autolink-headings": "^5.1.0",
    "rehype-slug": "^4.0.1",
    "remark-code-import": "^0.3.0",
    "remark-frontmatter": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-github": "^10.1.0",
    "remark-mdx-frontmatter": "^1.0.1",
    "remark-mdx-images": "^1.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "slugify": "^1.6.3"
  },
  "license": "MIT",
  "devDependencies": {
    "@supercharge/promise-pool": "^2.4.0",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.14.31",
    "autoprefixer": "^10.2.4",
    "canvas": "^2.11.2",
    "dotenv-safe": "^8.2.0",
    "eslint-config-next": "^13.5.5",
    "glob": "^10.3.10",
    "globby": "^13.1.1",
    "postcss": "^8.4.31",
    "puppeteer": "^19.7.2",
    "react-markdown": "^8.0.3",
    "ts-node": "^10.9.1"
  }
}
