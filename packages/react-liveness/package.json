{
  "name": "@aws-amplify/ui-react-liveness",
  "version": "3.3.8",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "types": "dist/types/index.d.ts",
  "sideEffects": [
    "dist/**/*.css"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react-liveness"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn build:rollup",
    "build:rollup": "rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-liveness\"'",
    "clean": "rimraf dist node_modules",
    "dev": "yarn build:rollup --watch",
    "lint": "yarn typecheck && eslint .",
    "prebuild": "rimraf dist",
    "size": "yarn size-limit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@aws-amplify/core": "*",
    "aws-amplify": "rn-exports",
    "react": "^16.14 || ^17 || ^18 || ^19",
    "react-dom": "^16.14 || ^17 || ^18 || ^19"
  },
  "dependencies": {
    "@aws-amplify/ui": "6.10.2",
    "@aws-amplify/ui-react": "6.11.1",
    "@aws-sdk/client-rekognitionstreaming": "3.621.0",
    "@aws-sdk/util-format-url": "3.609.0",
    "@smithy/eventstream-serde-browser": "^2.0.4",
    "@smithy/fetch-http-handler": "^2.1.3",
    "@smithy/protocol-http": "^3.0.3",
    "@smithy/signature-v4": "2.1.4",
    "@smithy/types": "^3.3.0",
    "@mediapipe/face_detection": "~0.4.0",
    "@tensorflow-models/face-detection": "1.0.2",
    "@tensorflow/tfjs-backend-cpu": "4.11.0",
    "@tensorflow/tfjs-backend-wasm": "4.11.0",
    "@tensorflow/tfjs-converter": "4.11.0",
    "@tensorflow/tfjs-core": "4.11.0",
    "@xstate/react": "^3.2.2",
    "nanoid": "3.3.8",
    "tslib": "^2.5.2",
    "xstate": "^4.33.6"
  },
  "devDependencies": {
    "@types/jest-when": "^3.5.0",
    "blob-polyfill": "^7.0.20220408",
    "jest-canvas-mock": "^2.5.2",
    "jest-websocket-mock": "^2.4.1",
    "jest-when": "^3.5.1",
    "mock-socket": "^9.2.1",
    "web-streams-polyfill": "^3.2.1"
  },
  "size-limit": [
    {
      "name": "FaceLivenessDetector",
      "path": "dist/esm/index.mjs",
      "import": "{ FaceLivenessDetector }",
      "limit": "225 kB"
    }
  ]
}