{
  "name": "@aws-amplify/ui-react-notifications",
  "version": "2.0.18",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react-notifications"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn build:rollup",
    "build:rollup": "rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-notifications\"'",
    "clean": "rimraf dist node_modules",
    "dev": "yarn build:rollup --watch",
    "lint": "yarn typecheck && eslint src --ext .js,.ts,.tsx",
    "prebuild": "rimraf dist",
    "size": "yarn size-limit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui": "6.0.15",
    "@aws-amplify/ui-react": "6.1.10",
    "@aws-amplify/ui-react-core-notifications": "2.0.15",
    "tinycolor2": "1.4.2"
  },
  "peerDependencies": {
    "aws-amplify": "^6.2.0",
    "react": "^16.14.0 || ^17.0 || ^18.0",
    "react-dom": "^16.14.0 || ^17.0 || ^18.0"
  },
  "devDependencies": {
    "@types/tinycolor2": "^1.4.3"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "size-limit": [
    {
      "name": "InAppMessaging",
      "path": "dist/esm/index.mjs",
      "import": "{ InAppMessagingProvider, InAppMessageDisplay }",
      "limit": "27 kB"
    }
  ]
}
