{
  "name": "@aws-amplify/ui-react-notifications",
  "version": "2.2.12",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react-notifications"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn build:rollup",
    "build:rollup": "yarn g:rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-notifications\"'",
    "clean": "yarn g:rimraf dist node_modules",
    "dev": "yarn build:rollup --watch",
    "lint": "yarn typecheck && eslint .",
    "prebuild": "yarn g:rimraf dist",
    "size": "yarn size-limit",
    "test": "yarn g:jest",
    "test:watch": "yarn test --watch",
    "typecheck": "yarn g:tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui": "6.12.0",
    "@aws-amplify/ui-react": "6.13.0",
    "@aws-amplify/ui-react-core": "3.4.5",
    "@aws-amplify/ui-react-core-notifications": "2.2.11",
    "tinycolor2": "1.4.2"
  },
  "peerDependencies": {
    "aws-amplify": "^6.14.3",
    "react": "^16.14 || ^17 || ^18 || ^19",
    "react-dom": "^16.14 || ^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@types/tinycolor2": "^1.4.3"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "size-limit": [
    {
      "name": "InAppMessaging",
      "path": "dist/esm/index.mjs",
      "import": "{ InAppMessagingProvider, InAppMessageDisplay }",
      "limit": "23 kB"
    }
  ]
}
