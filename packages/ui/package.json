{
  "name": "@aws-amplify/ui",
  "version": "3.0.1-next.1",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": "./dist/esm/index.js",
    "./styles.css": "./dist/styles.css",
    "./theme.css": "./dist/theme.css"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn build:sd && yarn build:css && yarn build:js",
