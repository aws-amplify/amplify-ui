    "@types/webpack-env": "^1.16.2",
    "autoprefixer": "^10.3.1",
    "change-case": "^4.1.2",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "globcat": "^1.3.4",
    "postcss": "^8.3.6",
    "rimraf": "^3.0.2",
    "sass": "^1.35.2",
    "style-dictionary": "^3.0.1",
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "aws-amplify": "3.x.x || 4.x.x"
  }
