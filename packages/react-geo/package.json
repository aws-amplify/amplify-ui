{
  "name": "@aws-amplify/ui-react-geo",
  "version": "1.0.2",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react-geo"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn build:rollup",
    "build:rollup": "rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-geo\"'",
    "clean": "rimraf dist node_modules",
    "dev": "yarn build:rollup --watch",
    "lint": "yarn typecheck && eslint src --ext .js,.ts,.tsx",
    "prebuild": "rimraf dist",
    "size": "yarn size-limit",
    "test": "jest",
    "test:ci": "yarn test && yarn check:esm",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "mapbox-gl": "1.13.1",
    "maplibre-gl": "2.1.9",
    "maplibre-gl-js-amplify": "^3.1.0",
    "react-map-gl": "7.0.23",
    "tslib": "^2.5.2"
  },
  "peerDependencies": {
    "aws-amplify": "^5.0.1",
    "react": "^16.14.0 || ^17.0 || ^18.0",
    "react-dom": "^16.14.0 || ^17.0 || ^18.0"
  },
  "devDependencies": {
    "@types/mapbox__mapbox-gl-draw": "^1.3.3"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "size-limit": [
    {
      "name": "Geo",
      "path": "dist/esm/index.mjs",
      "import": "{ MapView, LocationSearch }",
      "limit": "330 kB"
    }
  ]
}
