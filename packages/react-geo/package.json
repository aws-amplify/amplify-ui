{
  "name": "@aws-amplify/ui-react-geo",
  "version": "2.2.8",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react-geo"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn build:rollup",
    "build:rollup": "rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-geo\"'",
    "clean": "rimraf dist node_modules",
    "dev": "yarn build:rollup --watch",
    "lint": "yarn typecheck && eslint .",
    "prebuild": "rimraf dist",
    "size": "yarn size-limit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui-react-core": "3.4.2",
    "mapbox-gl": "1.13.1",
    "maplibre-gl": "2.1.9",
    "maplibre-gl-js-amplify": "^4.0.2",
    "react-map-gl": "7.0.23",
    "tslib": "^2.5.2"
  },
  "peerDependencies": {
    "@aws-amplify/geo": "^3.0.59",
    "aws-amplify": "rn-exports",
    "react": "^16.14 || ^17 || ^18 || ^19",
    "react-dom": "^16.14 || ^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@aws-amplify/geo": "^3.0.59",
    "@types/mapbox__mapbox-gl-draw": "^1.3.3"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "size-limit": [
    {
      "name": "LocationSearch",
      "path": "dist/esm/index.mjs",
      "import": "{ LocationSearch }",
      "limit": "202.5 kB"
    },
    {
      "name": "MapView",
      "path": "dist/esm/index.mjs",
      "import": "{ MapView }",
      "limit": "202 kB"
    }
  ]
}