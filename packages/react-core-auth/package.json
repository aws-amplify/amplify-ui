{
  "name": "@aws-amplify/ui-react-core-auth",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "react-native": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "LICENSE",
    "src",
    "!**/__mocks__",
    "!**/__tests__"
  ],
  "scripts": {
    "build": "rollup --config",
    "clean": "rimraf dist node_modules",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-auth-core\"'",
    "dev": "yarn build --watch",
    "lint": "yarn typecheck && eslint src",
    "prebuild": "rimraf dist",
    "test": "jest",
    "test:ci": "yarn test && yarn check:esm",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui": "5.8.1",
    "@aws-amplify/ui-react-core": "2.1.33",
    "@xstate/react": "3.0.1",
    "lodash": "4.17.21",
    "xstate": "^4.33.6"
  },
  "peerDependencies": {
    "aws-amplify": "console-preview",
    "react": ">= 16.14.0"
  },
  "devDependencies": {
    "@aws-amplify/eslint-config-amplify-ui": "0.0.0",
    "@aws-amplify/typescript-config": "0.0.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.2.22",
    "@types/react-native": "^0.71.3",
    "@types/react-dom": "^18.2.7",
    "@types/react-test-renderer": "^18.0.2",
    "eslint": "^8.44.0",
    "jest": "^27.0.4",
    "prettier": "2.4.1",
    "qrcode": "1.5.0",
    "react": "^18.2.22",
    "react-dom": "^18.2.22",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.0",
    "rollup-plugin-node-externals": "^4.1.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.3.2"
  },
  "sideEffects": false
}
