{
  "name": "@aws-amplify/ui-test-utils",
  "private": true,
  "version": "0.0.10",
  "description": "Test utils for Amplify Connected Component e2e tests",
  "exports": {
    "./*": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/*.mjs",
      "require": "./dist/*.js",
      "react-native": "./src/*/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn prebuild && yarn build:dist",
    "build:dist": "yarn g:rollup --config",
    "clean": "yarn g:rimraf dist node_modules",
    "dev": "yarn build --watch",
    "lint": "yarn typecheck && eslint src --ext .js,.ts,.tsx",
    "prebuild": "yarn g:rimraf dist",
    "typecheck": "yarn g:tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui-react-storage": "3.13.0",
    "tslib": "^2.5.2"
  }
}
