{
	"name": "@aws-amplify/ui-svelte",
	"version": "2.0.0",
	"type": "module",
	"main": "package/index.js",
	"exports": {
		".": {
			"import": "./package/index.js"
		}
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
