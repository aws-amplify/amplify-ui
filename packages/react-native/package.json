{
  "name": "@aws-amplify/ui-react-native",
  "version": "2.6.2",
  "main": "lib/index.js",
  "module": "dist/index.js",
  "react-native": "src/index.ts",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn prebuild && yarn build:cjs && yarn build:esm",
    "build:cjs": "yarn g:tsc --project tsconfig.cjs.json && cp -a src/assets/icons lib/assets",
    "build:esm": "yarn g:tsc --project tsconfig.esm.json && cp -a src/assets/icons dist/assets",
    "clean": "yarn g:rimraf dist",
    "dev": "yarn g:tsc --watch",
    "lint": "yarn typecheck && eslint src",
    "prebuild": "yarn g:rimraf dist",
    "test": "yarn g:jest",
    "test:watch": "yarn test --watch",
    "typecheck": "yarn g:tsc --noEmit"
  },
  "devDependencies": {
    "@aws-amplify/react-native": "^1.0.1",
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@react-native-async-storage/async-storage": "*",
    "@testing-library/react-native": "^12.9.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-native-safe-area-context": "^5.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-amplify/ui": "6.12.0",
    "@aws-amplify/ui-react-core": "3.4.5",
    "@aws-amplify/ui-react-core-notifications": "2.2.11"
  },
  "peerDependencies": {
    "aws-amplify": "^6.14.3",
    "react": "*",
    "react-native": ">=0.70",
    "react-native-safe-area-context": "*"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "!**/__mocks__",
    "!**/__tests__"
  ]
}
