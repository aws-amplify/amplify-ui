{
  "name": "@aws-amplify/ui-react-native",
  "version": "1.2.28",
  "main": "lib/index.js",
  "module": "dist/index.js",
  "react-native": "src/index.ts",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json && cp -a src/assets/icons lib/assets",
    "build:esm": "tsc --project tsconfig.esm.json && cp -a src/assets/icons dist/assets",
    "clean": "rimraf dist",
    "dev": "yarn build:esm --watch",
    "lint": "yarn typecheck && eslint src",
    "prebuild": "rimraf dist",
    "test": "jest",
    "test:ci": "yarn test",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@aws-amplify/eslint-config-amplify-ui": "0.0.0",
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.22.15",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.3.0",
    "@types/react": "^18.2.22",
    "@types/react-test-renderer": "^18.0.2",
    "babel-jest": "^28.0.3",
    "eslint": "^8.44.0",
    "jest": "^28",
    "metro-react-native-babel-preset": "^0.70.2",
    "react": "18.2.0",
    "react-native": "0.71.13",
    "react-native-safe-area-context": "^4.7.2",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28"
  },
  "dependencies": {
    "@aws-amplify/ui": "5.8.1",
    "@aws-amplify/ui-react-core": "2.1.33",
    "@aws-amplify/ui-react-core-notifications": "1.0.10"
  },
  "peerDependencies": {
    "aws-amplify": "^5.0.1",
    "react": ">= 16.14.0",
    "react-native": ">= 0.63.4",
    "react-native-safe-area-context": ">= 4.2.5"
  },
  "resolutions": {
    "@types/react": "^18.2.22"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "!**/__mocks__",
    "!**/__tests__"
  ]
}
