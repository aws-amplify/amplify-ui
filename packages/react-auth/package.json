{
  "name": "@aws-amplify/ui-react-auth",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react-auth"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn build:rollup",
    "build:rollup": "rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-auth\"'",
    "clean": "rimraf dist node_modules",
    "dev": "yarn build:rollup --watch",
    "lint": "yarn typecheck && eslint src --ext .js,.ts,.tsx",
    "prebuild": "rimraf dist",
    "size": "yarn size-limit",
    "test": "jest",
    "test:ci": "yarn test && yarn check:esm",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui": "5.8.1",
    "@aws-amplify/ui-react": "5.3.1",
    "@aws-amplify/ui-react-core": "2.1.33",
    "@aws-amplify/ui-react-core-auth": "0.0.1",
    "@xstate/react": "3.0.1",
    "classnames": "2.3.1",
    "lodash": "4.17.21",
    "tslib": "2.4.1"
  },
  "peerDependencies": {
    "aws-amplify": "^5.0.1",
    "react": "^16.14.0 || ^17.0 || ^18.0",
    "react-dom": "^16.14.0 || ^17.0 || ^18.0"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "size-limit": [
    {
      "name": "Authenticator",
      "path": "dist/esm/index.mjs",
      "import": "{ Authenticator }",
      "limit": "120 kB"
    }
  ]
}
