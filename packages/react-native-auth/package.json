{
  "name": "@aws-amplify/ui-react-native-auth",
  "version": "0.0.1",
  "main": "lib/index.js",
  "module": "dist/index.js",
  "react-native": "src/index.ts",
  "types": "dist/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "clean": "rimraf dist",
    "dev": "yarn build:esm --watch",
    "lint": "yarn typecheck && eslint src",
    "prebuild": "rimraf dist",
    "test": "jest",
    "test:ci": "yarn test",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@testing-library/react-native": "^12.3.0",
    "metro-react-native-babel-preset": "^0.70.2"
  },
  "dependencies": {
    "@aws-amplify/ui": "5.8.1",
    "@aws-amplify/ui-react-core-auth": "0.0.1",
    "@aws-amplify/ui-react-native": "1.2.28"
  },
  "peerDependencies": {
    "aws-amplify": "^5.0.1",
    "react": "^18",
    "react-native": "^0.70 || ^0.71 || ^0.72"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "!**/__mocks__",
    "!**/__tests__"
  ]
}
