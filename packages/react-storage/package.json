{
  "name": "@aws-amplify/ui-react-storage",
  "version": "3.10.2",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./browser": {
      "main": "./dist/browser.js",
      "types": "./dist/types/components/StorageBrowser/index.d.ts",
      "import": "./dist/esm/browser.mjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react-storage"
  },
  "files": [
    "dist",
    "LICENSE",
    "browser"
  ],
  "scripts": {
    "build": "yarn build:rollup",
    "build:rollup": "rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-storage\"'",
    "clean": "rimraf dist node_modules",
    "dev": "yarn build:rollup --watch",
    "lint": "yarn typecheck && eslint .",
    "prebuild": "rimraf dist",
    "size": "yarn size-limit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui": "6.10.2",
    "@aws-amplify/ui-react": "6.11.2",
    "@aws-amplify/ui-react-core": "3.4.2",
    "tslib": "^2.5.2"
  },
  "peerDependencies": {
    "aws-amplify": "^6.14.3",
    "react": "^16.14 || ^17 || ^18 || ^19",
    "react-dom": "^16.14 || ^17 || ^18 || ^19"
  },
  "devDependencies": {
    "@types/node": "^18.19.50"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "size-limit": [
    {
      "name": "createStorageBrowser",
      "path": "dist/esm/browser.mjs",
      "import": "{ createStorageBrowser }",
      "limit": "64.5 kB",
      "ignore": [
        "@aws-amplify/storage"
      ]
    },
    {
      "name": "StorageBrowser",
      "path": "dist/esm/index.mjs",
      "import": "{ StorageBrowser }",
      "limit": "87 kB"
    },
    {
      "name": "FileUploader",
      "path": "dist/esm/index.mjs",
      "import": "{ FileUploader }",
      "limit": "22 kB"
    },
    {
      "name": "StorageImage",
      "path": "dist/esm/index.mjs",
      "import": "{ StorageImage }",
      "limit": "14.5 kB"
    },
    {
      "name": "StorageManager",
      "path": "dist/esm/index.mjs",
      "import": "{ StorageManager }",
      "limit": "22 kB"
    }
  ]
}
