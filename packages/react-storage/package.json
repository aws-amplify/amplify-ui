{
  "name": "@aws-amplify/ui-react-storage",
  "version": "3.3.5",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react-storage"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn build:rollup",
    "build:rollup": "rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-storage\"'",
    "clean": "rimraf dist node_modules",
    "dev": "yarn build:rollup --watch",
    "lint": "yarn typecheck && eslint .",
    "prebuild": "rimraf dist",
    "size": "yarn size-limit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui": "6.6.1",
    "@aws-amplify/ui-react": "6.5.1",
    "@aws-amplify/ui-react-core": "3.0.25",
    "lodash": "4.17.21",
    "tslib": "^2.5.2"
  },
  "peerDependencies": {
    "@aws-amplify/core": "^6.4.0",
    "aws-amplify": "^6.6.0",
    "react": "^16.14.0 || ^17.0 || ^18.0",
    "react-dom": "^16.14.0 || ^17.0 || ^18.0"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "size-limit": [
    {
      "name": "FileUploader",
      "path": "dist/esm/index.mjs",
      "import": "{ FileUploader }",
      "limit": "21.5 kB"
    },
    {
      "name": "StorageImage",
      "path": "dist/esm/index.mjs",
      "import": "{ StorageImage }",
      "limit": "14.5 kB"
    },
    {
      "name": "StorageManager",
      "path": "dist/esm/index.mjs",
      "import": "{ StorageManager }",
      "limit": "21.5 kB"
    }
  ]
}
