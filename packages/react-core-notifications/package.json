{
  "name": "@aws-amplify/ui-react-core-notifications",
  "version": "2.2.9",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react-core-notifications"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn build:rollup",
    "build:rollup": "rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-core-notifications\"'",
    "clean": "rimraf dist node_modules",
    "dev": "yarn build:rollup --watch",
    "lint": "yarn typecheck && eslint .",
    "prebuild": "rimraf dist",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui": "6.10.3",
    "@aws-amplify/ui-react-core": "3.4.3"
  },
  "peerDependencies": {
    "aws-amplify": "^6.14.3",
    "react": "^16.14 || ^17 || ^18 || ^19"
  },
  "sideEffects": false
}