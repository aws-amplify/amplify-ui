{
  "name": "@aws-amplify/ui-react-core",
  "version": "2.1.33",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "react-native": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist node_modules",
    "build": "rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react-core\"'",
    "dev": "yarn build --watch",
    "lint": "yarn typecheck && eslint src",
    "prebuild": "rimraf dist",
    "test": "jest",
    "test:ci": "yarn test && yarn check:esm",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui": "5.8.1",
    "@xstate/react": "^3.2.2",
    "lodash": "4.17.21",
    "xstate": "^4.33.6"
  },
  "peerDependencies": {
    "aws-amplify": "^5.0.1",
    "react": ">= 16.14.0"
  },
  "devDependencies": {
    "@aws-amplify/eslint-config-amplify-ui": "0.0.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^17.0.13",
    "@types/react-test-renderer": "^18.0.2",
    "eslint": "^8.44.0",
    "jest": "^28",
    "jest-environment-jsdom": "^28",
    "prettier": "2.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.0",
    "rollup-plugin-node-externals": "^4.1.1",
    "ts-jest": "^28",
    "typescript": "^4.3.2"
  },
  "sideEffects": false
}
