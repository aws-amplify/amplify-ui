{
  "name": "@aws-amplify/ui-react",
  "version": "6.11.2",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/index.js"
    },
    "./internal": {
      "import": "./dist/esm/internal.mjs",
      "require": "./dist/internal.js",
      "types": "./dist/types/internal.d.ts"
    },
    "./server": {
      "types": "./dist/types/server.d.ts",
      "import": "./dist/esm/server.mjs",
      "require": "./dist/server.js"
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css",
    "./styles/*": "./dist/styles/*",
    "./primitives.json": "./dist/primitives.json"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react"
  },
  "files": [
    "dist",
    "internal",
    "server",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn build:catalog && yarn build:CSS && yarn build:rollup",
    "build:catalog": "yarn node -r esbuild-register scripts/generatePrimitiveCatalog.ts",
    "build:CSS": "yarn node -r esbuild-register scripts/copyCSS.ts",
    "build:rollup": "rollup --config",
    "check:esm": "node --input-type=module --eval 'import \"@aws-amplify/ui-react\"'",
    "clean": "rimraf dist node_modules",
    "dev": "yarn build:rollup --watch",
    "lint": "yarn typecheck && eslint .",
    "prebuild": "rimraf dist",
    "size": "yarn size-limit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui": "6.10.3",
    "@aws-amplify/ui-react-core": "3.4.3",
    "@radix-ui/react-direction": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.10",
    "@radix-ui/react-slider": "^1.3.2",
    "@xstate/react": "^3.2.2",
    "lodash": "4.17.21",
    "qrcode": "1.5.0",
    "tslib": "^2.5.2"
  },
  "peerDependencies": {
    "@aws-amplify/core": "*",
    "aws-amplify": "^6.14.3",
    "react": "^16.14.0 || ^17.0 || ^18.0 || ^19",
    "react-dom": "^16.14 || ^17 || ^18 || ^19"
  },
  "peerDependenciesMeta": {
    "aws-amplify": {
      "optional": true
    }
  },
  "devDependencies": {
    "@svgr/core": "^5.5.0",
    "@types/qrcode": "^1.4.2",
    "jest-matchmedia-mock": "^1.1.0"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "size-limit": [
    {
      "name": "Authenticator",
      "path": "dist/esm/index.mjs",
      "import": "{ Authenticator }",
      "limit": "65 kB"
    },
    {
      "name": "AccountSettings",
      "path": "dist/esm/index.mjs",
      "import": "{ AccountSettings }",
      "limit": "25 kB"
    }
  ]
}
