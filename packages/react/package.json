{
  "name": "@aws-amplify/ui-react",
  "version": "4.3.2",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./internal": {
      "import": "./dist/esm/internal.js",
      "require": "./dist/internal.js"
    },
    "./styles.css": "./dist/styles.css",
    "./primitives.json": "./dist/primitives.json"
  },
  "browser": {
    "./styles.css": "./dist/styles.css"
  },
  "types": "dist/types/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-ui",
    "directory": "packages/react"
  },
  "files": [
    "dist",
    "internal",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn run build:catalog && yarn run build:ts",
    "build:ts": "rollup --config",
    "build:catalog": "yarn ts scripts/generatePrimitiveCatalog.ts",
    "dev": "tsup --watch",
    "dev:build": "tsup",
    "clean": "rimraf dist node_modules",
    "lint": "tsc --noEmit --project tsconfig.dev.json && eslint src --ext .js,.ts,.tsx",
    "test": "yarn test:unit",
    "test:watch": "yarn test:unit:watch",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "ts": "node -r esbuild-register",
    "size": "yarn run size-limit"
  },
  "dependencies": {
    "@aws-amplify/ui": "5.4.2",
    "@aws-amplify/ui-react-core": "2.1.7",
    "@radix-ui/react-accordion": "1.0.0",
    "@radix-ui/react-direction": "1.0.0",
    "@radix-ui/react-dropdown-menu": "1.0.0",
    "@radix-ui/react-slider": "1.0.0",
    "@radix-ui/react-tabs": "1.0.0",
    "@xstate/react": "3.0.0",
    "classnames": "2.3.1",
    "deepmerge": "4.2.2",
    "lodash": "4.17.21",
    "mapbox-gl": "1.13.1",
    "maplibre-gl": "2.1.9",
    "maplibre-gl-js-amplify": "3.0.0",
    "qrcode": "1.5.0",
    "react-generate-context": "1.0.1",
    "react-map-gl": "7.0.15",
    "tinycolor2": "1.4.2",
    "tslib": "2.4.1"
  },
  "peerDependencies": {
    "aws-amplify": "5.x.x",
    "react": ">= 16.14.0",
    "react-dom": ">= 16.14.0"
  },
  "peerDependenciesMeta": {
    "aws-amplify": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-typescript": "^8.3.1",
    "@size-limit/preset-big-lib": "^7.0.8",
    "@svgr/core": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.23",
    "@types/qrcode": "^1.4.2",
    "@types/react": "^17.0.2",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "degit": "^2.8.4",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "jest": "^27.0.4",
    "jest-matchmedia-mock": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.0",
    "rollup-plugin-node-externals": "^4.1.1",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.0.3",
    "ts-morph": "^12.0.0"
  },
  "sideEffects": [
    "dist/**/*.css"
  ],
  "size-limit": [
    {
      "name": "Authenticator",
      "path": "dist/esm/index.js",
      "import": "{ Authenticator }",
      "limit": "120 kB"
    },
    {
      "name": "Geo",
      "path": "dist/esm/index.js",
      "import": "{ MapView, LocationSearch }",
      "limit": "330 kB"
    }
  ]
}
